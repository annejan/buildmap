server {
        listen 80 default_server;
        listen [::]:80 default_server;

        root /home/vagrant/map.emfcamp.org;

        index index.html;

        server_name _;

        location / {
                try_files $uri $uri/ =404;
                autoindex on;
        }

        location /tiles {
                rewrite /tiles(.*) $1  break;
                proxy_pass http://127.0.0.1:8080;
        }

}
